{
    "openapi": "3.0.2",
    "info": {
        "title": "Simple Data Type Sample API",
        "version": "1.0.0"
    },
    "paths": {
        "/widgets/{widgetId}": {
            "get": {
                "responses": {
                    "200": {
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "string",
                                    "format": "binary"
                                }
                            }
                        },
                        "description": "Returns the list of widgets."
                    }
                },
                "operationId": "getWidgets",
                "summary": "Get widgets"
            },
            "parameters": [
                {
                    "name": "widgetId",
                    "schema": {
                        "$ref": "#/components/schemas/MyId"
                    },
                    "in": "path",
                    "required": true
                }
            ]
        }
    },
    "components": {
        "schemas": {
            "MyId": {
                "description": "",
                "type": "string",
                "example": "\"hello world\""
            },
            "MyMap": {
                "type": "object",
                "additionalProperties": {
                    "type": "string"
                },
                "x-codegen-type": "StringMap",
                "x-codegen-inline": "true"
            },
            "MyQuarkusBean": {
            	"type": "object",
            	"properties": {
                    "name": {
                        "type": "string"
                    },
                    "description": {
                        "type": "string"
                    }
            	},
            	"x-codegen-annotations": ["io.quarkus.runtime.annotations.RegisterForReflection"]
            },
            "SampleDataType": {
                "description": "",
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string"
                    },
                    "mylong": {
                        "type": "integer",
                        "format": "int64"
                    },
                    "myint": {
                        "type": "integer",
                        "format": "int32"
                    },
                    "mydate": {
                    	"type": "string",
                    	"format": "date-time"
                    },
                    "mymap": {
                        "$ref": "#/components/schemas/MyMap"
                    },
                    "id": {
                        "$ref": "#/components/schemas/MyId"
                    }
                }
            }
        }
    }
}