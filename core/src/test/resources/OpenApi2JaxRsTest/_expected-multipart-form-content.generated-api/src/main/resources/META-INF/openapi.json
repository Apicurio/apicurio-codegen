{
  "openapi": "3.0.3",
  "info": {
    "title": "Example API",
    "version": "1.0.0"
  },
  "paths": {
    "/files/postFile": {
      "post": {
        "requestBody": {
          "required": true,
          "content": {
            "multipart/form-data": {
              "schema": {
                "type": "object",
                "properties": {
                  "file": {
                    "type": "string",
                    "format": "binary"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/FileUploadProcessResponse"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "SomeObject": {
        "type": "object",
        "properties": {
          "someList": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Some-Other-Object"
            }
          }
        }
      },
      "Some-Other-Object": {
        "type": "object",
        "properties": {
          "exampleProperty": {
            "type": "string"
          }
        }
      },
      "FileUploadProcessResponse": {
        "type": "object",
        "properties": {
          "message": {
            "type": "string"
          },
          "success": {
            "type": "boolean"
          }
        }
      }
    }
  }
}
